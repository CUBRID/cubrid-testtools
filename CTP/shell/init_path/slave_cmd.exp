#!/usr/bin/expect

set HOST [lindex $argv 0]
set PASSWORD [lindex $argv 1]
set timeout 60

spawn ssh $HOST
expect {
"(yes/no)?" {send "yes\r";exp_continue}
"password:" {send "$PASSWORD\r"}
}
expect "$"

set x 2 
while {$x < $argc} {
  send [lindex $argv $x]
  send "\r"
  expect "$"
  set x [expr {$x + 1}]
}

expect "$"
send "exit\r"
expect eof

